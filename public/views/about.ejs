<%layout('layouts/boilerplate')%>
<% block('head').append('<title>Category</title>') %>
<% block('head').append('<link rel="stylesheet" href="../styles/about.css" />') %>

<h1>
    <%=category%> Category
</h1>
<h2>
    Services
</h2>
<ul>
    <% foundServices.forEach(service=> { %>
        <li>
            <a href="/services/<%=category%>/<%=service.name %>">
                <%=service.name %>
            </a>
        </li>
        <%}) %>
</ul>
<a href="/services">Back to Services</a>

